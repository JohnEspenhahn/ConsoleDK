{
  "name": "app",
  "version": "0.1.0",
  "bin": {
    "app": "build/app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc-watch --onSuccess \"npm run synth\"",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "npm run build && cdk deploy --require-approval never",
    "recursive-install": "npm-recursive-install",
    "synth": "npm run build && cdk synth --quiet",
    "python-server": "python -m http.server --directory build/console/",
    "server": "concurrently \"npm run watch\" \"npm run python-server\""
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.117.0",
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "@types/react": "^17.0.16",
    "@types/react-dom": "^17.0.9",
    "@types/react-native": "^0.64.12",
    "@types/uuid": "^8.3.1",
    "aws-cdk": "1.117.0",
    "babel-loader": "^8.2.2",
    "jest": "^26.4.2",
    "rollup": "^2.56.0",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "tsc-watch": "^4.4.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@babel/preset-react": "^7.14.5",
    "@types/aws-lambda": "^8.10.83",
    "@types/webpack": "^5.28.0",
    "aws-embedded-metrics": "^2.0.4",
    "aws-sdk": "^2.963.0",
    "aws-xray-sdk": "^3.3.3",
    "co": "^4.6.0",
    "concurrently": "^6.2.1",
    "core-js": "^3.16.0",
    "csv-parser": "^3.0.0",
    "html-webpack-plugin": "^5.3.2",
    "lodash": "^4.17.21",
    "monocdk": "^1.117.0",
    "node-sql-parser": "^3.6.5",
    "react": "^17.0.2",
    "react-art": "^17.0.2",
    "react-data-grid": "^7.0.0-beta.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.1",
    "source-map-support": "^0.5.16",
    "ts-loader": "^9.2.5",
    "webpack": "^5.49.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
